<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="keywords" content="'<%= title %>'" />
    <meta name="description" content="'<%= title %>'" />
    <title>
        <%= objdetail.Title%>
    </title>
    <link rel="stylesheet" href="./stylesheets/css/entrene.css">
    <script src="./javascripts/zepto.js"></script>
    <script src="./javascripts/es6js/common.js"></script>
    <style>
         .tip p {
            margin-bottom: 15px;
        }
    </style>
    <link rel="canonical" href="http://m.itenbo.com/entreneurship" />
</head>

<body>
    <div class="entrene-wrappper">
        <div class="entrene-content">
            <h3><%= objdetail.Title%></h3>
            <div class="autho">
                <span><%= objdetail.Author%></span>
                <i></i>
                <span><%= objdetail.PublishDate%></span>
            </div>
            <p></p>
            <div class="image"></div>
            <div class="tip"style="font-size: 14px; color: #808080; line-height: 25px;">
                <%- objdetail.Content%>
            </div>
        </div>
    </div>
</body>
<script>
    $(function(){
        function isWeiXin(){ 
            var ua = window.navigator.userAgent.toLowerCase(); 
            if(ua.match(/MicroMessenger/i) == 'micromessenger'){ 
                return true; 
            }else{ 
                return false; 
            } 
        } 
        if(!isWeiXin()){
            var html2 = `<div style="width:100%;height:.5rem;"></div>`
            var html = `<div class='goback' style='border-bottom:1px solid rgb(227, 227, 227);height:.5rem;line-height:.5rem;;width:100%;background:#fff;text-align:center;font-size:.18rem;position:fixed;top:0;left:0;z-index:8'><i style='display:inline-block;height:100%;width:.4rem;position:absolute;left:0;background:url(../../images/goback.png) no-repeat center;background-size:.2rem .2rem;'></i>创业</div>`;
            $('body').prepend(html2);
            $('body').prepend(html);
            $('.headNews').css('top','.5rem');
            $('.goback i').off('click').on('click',function(){
                window.history.back();
            })
        }else {
            $('.headNews').css('top','0');
        };
    });
</script>
<script src="./javascripts/httpConfig2.js"></script>
<script src="./javascripts/baidu.js"></script>
</html>